{
  "name": "ordohub",
  "version": "1.0.0",
  "description": "Hub Universal de Gestão Doméstica Colaborativa",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:incremental": "tsc --noEmit --incremental",
    "prepare": "husky install",
    "start": "npm run dev",
    "clean": "rimraf dist node_modules/.cache",
    "clean:types": "rimraf node_modules/.cache/typescript",
    "setup:folders": "node scripts/setup-folders.js",
    "check": "npm run type-check && npm run lint && npm run format:check",
    "check:fix": "npm run type-check && npm run lint:fix && npm run format"
  },
  "keywords": [
    "home-management",
    "collaboration",
    "household",
    "task-management",
    "family",
    "organization",
    "hub",
    "domestic",
    "gestão-doméstica"
  ],
  "author": "Lucas Pinto <lucaspintomg@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucaspintomg/ordohub.git"
  },
  "bugs": {
    "url": "https://github.com/lucaspintomg/ordohub/issues"
  },
  "homepage": "https://github.com/lucaspintomg/ordohub#readme",
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@supabase/supabase-js": "^2.57.4",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "chrono-node": "^2.8.4",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "framer-motion": "^11.5.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.544.0",
    "mongoose": "^8.6.2",
    "multer": "^2.0.0-rc.4",
    "nanoid": "^5.1.5",
    "nodemailer": "^6.9.15",
    "react": "^18.3.1",
    "react-calendar": "^5.0.0",
    "react-datepicker": "^7.3.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "react-use": "^17.5.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^3.3.1",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "winston": "^3.14.2",
    "zod": "^4.1.7",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/cron": "^2.4.0",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.7",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.1",
    "@types/nodemailer": "^6.4.15",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.1",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "^2.1.9",
    "@vitest/ui": "^2.1.1",
    "autoprefixer": "^10.4.20",
    "c8": "^10.1.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^16.4.0",
    "husky": "^9.1.5",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.6",
    "vite-plugin-checker": "^0.10.3",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
