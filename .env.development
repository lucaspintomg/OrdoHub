# OrdoHub - Ambiente de Desenvolvimento Local
VITE_PORT=3000
VITE_HMR_PORT=24678
VITE_PREVIEW_PORT=4173

# API Local
VITE_API_URL=http://localhost:8000
VITE_WEBSOCKET_URL=ws://localhost:8000

# App Config
VITE_APP_NAME="OrdoHub Dev"
VITE_APP_ENVIRONMENT=development

# Features (Desenvolvimento)
VITE_FEATURE_PWA=true
VITE_FEATURE_ANALYTICS=false
VITE_FEATURE_NOTIFICATIONS=true
VITE_FEATURE_OFFLINE_MODE=true

# Debug
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
VITE_SHOW_DEV_TOOLS=true
VITE_MOCK_API=false

# OrdoHub Settings
ORDOHUB_FAMILY_MAX_MEMBERS=10
ORDOHUB_AUTO_SAVE_INTERVAL=30000
